(define
 (problem pfile_multi_tasks)
 (:domain robot)
 (:objects o1 - PACKAGE c r1 r2 r3 - ROOM d12 d13 d01 - ROOMDOOR)
 (:htn
  :ordered-tasks (and
    (task0 (achieve-goals arm1))
    (task1 (achieve-goals arm2))
  )
 )
 (:init
  (rloc c)
  (armempty)
  (door c r1 d01)
  (door r1 c d01)
  (door r1 r2 d12)
  (door r1 r3 d13)
  (door r2 r1 d12)
  (door r3 r1 d13)
  (closed d13)
  (in o1 r3)
  (goal_in o1 r2))
 (:goal (and
  (in o1 r2)
 ))
)
